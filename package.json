{
  "name": "walch-pizza",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist && mkdir dist",
    "stage:assets": "cp -r assets dist",
    "stage:markup": "cp src/*.html dist",
    "stage": "npm run stage:assets && npm run stage:markup",
    "compile:styles": "lessc src/styles.less dist/styles.css && lessc src/wedding.less dist/wedding.css && lessc src/home.less dist/home.css && lessc src/multimedia.less dist/multimedia.css && lessc src/venue.less dist/venue.css",
    "compile:scripts": "tsc ./src/*.ts --outDir ./dist",
    "compile:functions": "tsc ./netlify/functions/*.ts --outDir ./netlify/functions --esModuleInterop",
    "compile": "npm run compile:scripts && npm run compile:styles",
    "build": "npm run clean && npm run stage && npm run compile",
    "start:local": "cd ./dist && python -m SimpleHTTPServer 3001",
    "start": "npm run build && npm run start:local",
    "help": "lessc --help"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tywalch/walch-pizza.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tywalch/walch-pizza/issues"
  },
  "homepage": "https://github.com/tywalch/walch-pizza#readme",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.87.0",
    "@netlify/functions": "^1.0.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/moment": "^2.13.0",
    "aws-sdk": "^2.1135.0",
    "electrodb": "^1.8.1",
    "moment": "^2.29.3"
  },
  "devDependencies": {
    "@types/node": "^17.0.33",
    "less": "^4.1.2",
    "sass": "^1.51.0",
    "typescript": "^4.6.4"
  }
}
